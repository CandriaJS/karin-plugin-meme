{
  "name": "@candriajs/karin-plugin-meme",
  "version": "4.0.4",
  "description": "一个Karin版的表情包合成插件",
  "keywords": [
    "karin-plugin",
    "karin-plugin-meme"
  ],
  "homepage": "https://github.com/CandriaJS/karin-plugin-meme",
  "bugs": {
    "url": "https://github.com/CandriaJS/karin-plugin-meme/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CandriaJS/karin-plugin-meme.git"
  },
  "license": "GNU General Public License v3.0",
  "author": "CandriaJS",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "config/",
    "lib/",
    "resources/",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "app": "node --import tsx src/app.ts",
    "build": "tsup",
    "dev": "cross-env EBV_FILE=\"development.env\" node --import tsx src/app.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pub": "npm publish --access public",
    "sort": "sort-package-json",
    "watch": "cross-env EBV_FILE=\"development.env\" tsx watch --include \"src/**/*.ts\" src/app.ts"
  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "radashi": "^12.6.2",
    "sequelize": "^6.37.7",
    "meme-generator": "npm:@memecrafters/meme-generator@0.2.2-beta"
  },
  "devDependencies": {
    "@karinjs/puppeteer": "^2.2.3",
    "@types/adm-zip": "^0.5.7",
    "@types/lodash": "^4.17.16",
    "@types/markdown-it": "^14.1.2",
    "icqq": "^0.6.10"
  },
  "karin": {
    "main": "src/index.ts",
    "web": "./lib/web.config.js",
    "ts-web": "./src/web.config.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ]
  },
  "engines": {
    "karin": ">=1.10.1"
  },
  "packageManager": "pnpm@10.12.4"
}
